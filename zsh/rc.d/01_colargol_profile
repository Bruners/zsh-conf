# Extra exports to load after .zshenv

typeset -U EDITOR
EDITOR='/usr/bin/vim'
typeset -U PAGER
PAGER='/bin/more'
typeset -U MANPAGER
MANPAGER='/usr/bin/w3mman'

export EDITOR PAGER MANPAGER

HISTSIZE=500
SAVEHIST=500
HISTFILE='~/.zhistory'

export HISTSIZE SAVEHIST HISTFILE

export BROWSER='/usr/bin/w3m'

export TERM="screen-256color"

# vim:set ft=zsh:
